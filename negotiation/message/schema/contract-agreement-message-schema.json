{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"title": "ContractAgreementMessageSchema",
	"type": "object",
	"allOf": [
	  {
		"$ref": "#/definitions/ContractAgreementMessage"
	  }
	],
	"$id": "https://w3id.org/dspace/schemas/v0.8/ContractAgreementMessageSchema",
	"definitions": {
		"ContractAgreementMessage": {
			"type": "object",
			"properties": {
				"@context": {
					"type": "string",
					"const": "https://w3id.org/dspace/v0.8/context.json",
					"required": true
				},
				"@type": {
					"type": "string",
					"const": "dspace:ContractAgreementMessage",
					"required": true
				},
				"dspace:processId": {
					"type": "string",
					"required": true
				},
				"odrl:agreement": {
					"$ref": "https://w3id.org/dspace/schemas/v0.8/AgreementSchema#definitions/Agreement",
					"type": "object",
					"required": true,
					"properties": {
						"@type": {
							"type": "string",
							"required": true
						},
						"@id": {
							"type": "string",
							"required": true
						},
						"dspace:timestamp": {
							"type": "object",
							"required": true,
							"properties": {
								"@value": {
									"type": "string",
									"format": "date-time",
									"required": true
								},
								"@type": {
									"type": "string",
									"const": "xsd:dateTime",
									"required": true
								}
							}
						},
						"dspace:consumerId": {
							"type": "string",
							"required": true
						},
						"dspace:providerId": {
							"type": "string",
							"required": true
						}
					}
				}
			}
		}
	}
}