{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"title": "ContractAgreementMessageSchema",
	"type": "object",
	"allOf": [
	  {
		"$ref": "#/definitions/ContractAgreementMessage"
	  }
	],
	"$id": "https://w3id.org/dspace/schemas/v0.8/ContractAgreementMessageSchema",
	"definitions": {
		"ContractAgreementMessage": {
			"type": "object",
			"properties": {
				"@context": {
					"type": "string",
					"const": "https://w3id.org/dspace/v0.8/context.json"
				},
				"@type": {
					"type": "string",
					"const": "dspace:ContractAgreementMessage"
				},
				"dspace:processPid": {
					"type": "string"
				},
				"dspace:agreement": {
					"$ref": "https://w3id.org/dspace/schemas/v0.8/contract-schema.json#/definitions/Agreement",
					"type": "object",
					"properties": {
						"@type": {
							"type": "string"
						},
						"@id": {
							"type": "string"
						},
						"odrl:target": {
							"type": "object"
						},
						"dspace:timestamp": {
							"type": "object",
							"properties": {
								"@value": {
									"type": "string",
									"format": "date-time"
								},
								"@type": {
									"type": "string",
									"const": "xsd:dateTime"
								}
							},
							"required": [ "@value", "@type" ]
						},
						"dspace:consumerId": {
							"type": "string"
						},
						"dspace:providerId": {
							"type": "string"
						}
					},
					"required": [ "@type", "@id", "odrl:target", "dspace:timestamp", "dspace:consumerId", "dspace:providerId" ]
				},
				"dspace:callbackAddress": {
					"type" : "string"
				}
			},
			"required": [ "@context", "@type", "dspace:processPid", "dspace:agreement", "dspace:callbackAddress"]
		}
	}
}