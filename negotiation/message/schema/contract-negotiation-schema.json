{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"title": "ContractNegotiationSchema",
	"type": "object",
	"allOf": [
	  {
		"$ref": "#/definitions/ContractNegotiation"
	  }
	],
	"$id": "https://w3id.org/dspace/schemas/v0.8/ContractNegotiationSchema",
	"definitions": {
		"ContractNegotiation": {
			"type": "object",
