            "Distribution": {
                "type": "object",
                "properties": {
                    "dcat:accessService": {
                        "type": "array",
                        "items": {
                            "ref": "definitions/DataService"
                        },
                        "minItems": 1
                    },
                    "dcat:accessURL": {
                        "type": "string"
                    },
                    "dcat:byteSize": {
                        "type": "integer"
                    },
                    "dcat:compressFormat": {
