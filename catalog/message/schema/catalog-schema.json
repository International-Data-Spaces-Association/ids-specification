                    },
                    "dct:title": {
                        "type": "string"
                    },
                    "odrl:hasPolicy": {
                        "type": "boolean"
                    }
                }

            }			
        
		},
        "Dataset": {

        },
        "time": {
            "type": "object",
            "properties": {
                "@type": {
                    "type": "string",
                    "const": "xsd:date"
                },
                "@value": {
                    "type": "string",
                    "pattern": "-?([1-9][0-9]{3,}|0[0-9]{3})-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01])T(([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\\.[0-9]+)?|(24:00:00(\\.0+)?))(Z|(\\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00))?"
                }
            }
        }
	}
}