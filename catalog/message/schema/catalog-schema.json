{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"title": "CatalogSchema",
	"type": "object",
	"allOf": [
	  {
		"$ref": "#/definitions/Catalog"
	  }
	],
	"$id": "https://w3id.org/dspace/schemas/1/0/CatalogSchema",
	"definitions": {
		"Catalog": {
			"type": "object",
            "required":["@context", "@type", "dct:hasPart"],
			"properties": {
				"@context": {
					"type": "string",
					"const": "https://w3id.org/dspace/1/0/context.json"
				},
				"@type": {
					"type": "string",
					"const": "dcat:Catalog"
				},
				"dcat:catalog": {
                    "type": "array",
                    "items": {
						"ref": "definitions/Catalog"
                    },
                    "minItems": 1
				},
				"dcat:dataset": {
                    "type": "array",
                    "items": {
                        "ref": "definitions/Dataset"
                    },
                    "minItems": 1
				},
				"dcat:record": {
                    "ref": "definitions/CatalogRecord"                    
				},
                "dcat:service": {
                    "type": "array",
                    "items": {
						"ref": "definitions/DataService"
                    },
                    "minItems": 1
                },
                "dcat:themeTaxonomy": {
                    "type": "string"
                },
                "dct:hasPart": {
                    "type": "array",
                    "items": {
						"ref": "definitions/Resource"
                    },
                    "minItems": 1
                },
                "foaf:homepage": {
                    "type": "string"
                },
                "dcat:distribution": {
                    "type": "string"
                },
                "dcat:spatialResolutionInMeters": {
                    "type": "string"
                },
                "dcat:temporalResolution": {
                    "type": "string"
                },
                "dct:accrualPeriodicity": {
                    "type": "string"
                },
                "dct:spatial": {
                    "type": "string"
                },
                "dct:temporal": {
                    "type": "object",
                    "properties": {
                        "@type": {
                            "type": "string",
                            "const": "dcterms:PeriodOfTime"
                        },
                        "dcat:startDate": {
                            "ref": "definitions/time"
                        },
                        "dcat:endDate": {
                            "ref": "definitions/time"
                        }
                    }
                },
                "prov:wasGeneratedBy": {
                    "type": "string"
                },
                "dcat:contactPoint": {
                    "type": "string"
                },
                "dcat:keyword": {
                    "type": "string"
                },
                "dcat:landingPage": {
                    "type": "string"
                },
                "dcat:qualifiedRelation": {
                    "type": "string"
                },
                "dcat:theme": {
                    "type": "string"
                },
                "dct:accessRights": {
                    "type": "string"
                },
                "dct:conformsTo": {
                    "type": "string"
                },
                "dct:creator": {
                    "type": "string"
                },
                "dct:description": {
                    "type": "string"
                },
                "dct:identifier": {
                    "type": "string"
                },
                "dct:isReferencedBy": {
                    "type": "string"
                },
                "dct:issued": {
                    "type": "string"
                },
                "dct:language": {
                    "type": "string"
                },
                "dct:license": {
                    "type": "string"
                },
                "dct:modified": {
                    "type": "boolean"
                },
                "dct:publisher": {
                    "type": "string"
                },
                "dct:relation": {
                    "type": "string"
                },
                "dct:rights": {
                    "type": "string"
                },
                "dct:title": {
                    "type": "string"
                },
                "dct:type": {
                    "type": "string"
                },
                "odrl:hasPolicy": {
                    "type": "boolean"
                },
                "prov:qualifiedAttribution": {
                    "type": "string"
                }

			},
            "CatalogRecord":{
                "type": "object",
                "required":["foaf:primaryTopiy"],
                "properties": {
                    "@type": {
                        "type": "string",
                        "const": "dcat:CatalogRecord"
                    },
                    "dct:conformsTo": {
                        "type": "string"
                    },
                    "dct:description": {
                        "type": "string"
                    },
                    "dct:issued": {
                        "type": "string"
                    },
                    "dct:modified": {
                        "type": "boolean"
                    },
                    "dct:title": {
                        "type": "string"
                    },
                    "foaf:primaryTopic":{
                        "ref": "definitions/Resource"
                    }
                }
            },
            "Resource":{
                "type": "object",
                "properties": {
                    "dcat:contractPoint": {
                        "type": "string"
                    },
                    "dcat:keyword": {
                        "type": "string"
                    },
                    "dcat:landingPage": {
                        "type": "string"
                    },
                    "dcat:qualifiedRelation": {
                        "ref": "definitions/Relationship"
                    },
                    "dcat:theme": {
                        "type": "array",
                        "items": {
                            "ref": "definitions/Concept"									
                        },
                        "minItems": 1
                    },
                    "dct:accessRights": {
                        "type": "string"
                    },
                    "dct:conformsTo": {
                        "type": "string"
                    },
                    "dct:creator": {
                        "type": "string"
                    },
                    "dct:description": {
                        "type": "string"
                    },
                    "dct:identifier": {
                        "type": "string"
                    },
                    "dct:isReferencedBy": {
                        "type": "string"
                    },
                    "dct:issued": {
                        "type": "string"
                    },
                    "dct:language": {
                        "type": "string"
                    },
                    "dct:license": {
                        "type": "string"
                    },
                    "dct:modified": {
                        "type": "boolean"
                    },
                    "dct:publisher": {
                        "type": "string"
                    },
                    "dct:relation": {
                        "type": "string"
                    },
                    "dct:rights": {
                        "type": "string"
                    },
                    "dct:title": {
                        "type": "string"
                    },
                    "dct:type": {
                        "type": "string"
                    },
                    "odrl:hasPolicy": {
                        "type": "boolean"
                    },
                    "prov:qualifiedAttribution": {
                        "type": "string"
                    }
                }
             } ,
             "Relationship":{
                "type": "object",
                "const": "dcat:Relationship",
                "properties": {
                    "dcat:hadRole": {
                        "type": "boolean"
                    },
                    "dct:relation":{
                        "ref": "definitions/Resource"
                    }
                } 
             },
             "Concept": {
                "type": "object",
                "skos:inScheme": {
                    "ref": "definitions/Resource"
                }
             },
             "ConceptScheme": {
                "type": "object"
                
            },
            "Agent": {
                "type": "object"
            },
             
            "Dataset": {
                "type": "object",
                "properties": {
                    "dcat:distribution": {
                        "type": "array",
                        "items": {
                            "ref": "definitions/Distribution"
                        },
                        "minItems": 1
                        
                    },
                    "dcat:spatialResolutionInMeters": {
                        "type": "string"
                    },
                    "dcat:temporalResolution": {
                        "type": "string"
                    },
                    "dct:accrualPeriodicity": {
                        "type": "string"
                    },
                    "dct:spatial": {
                        "type": "string"
                    },
                    "dct:temporal": {
