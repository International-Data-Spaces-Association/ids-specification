                    },
                    "dct:accessRights": {
                        "type": "string"
                    },
                    "dct:conformsTo": {
                        "type": "string"
                    },
                    "dct:creator": {
                        "type": "string"
                    },
                    "dct:description": {
                        "type": "string"
                    },
                    "dct:identifier": {
                        "type": "string"
                    },
                    "dct:isReferencedBy": {
                        "type": "string"
                    },
                    "dct:issued": {
                        "type": "string"
                    },
                    "dct:language": {
                        "type": "string"
                    },
                    "dct:license": {
                        "type": "string"
                    },
                    "dct:modified": {
                        "type": "boolean"
                    },
                    "dct:publisher": {
                        "type": "string"
                    },
                    "dct:relation": {
                        "type": "string"
                    },
                    "dct:rights": {
                        "type": "string"
                    },
                    "dct:title": {
                        "type": "string"
                    },
                    "dct:type": {
                        "type": "string"
                    },
                    "odrl:hasPolicy": {
                        "type": "boolean"
                    },
                    "prov:qualifiedAttribution": {
                        "type": "string"
                    }
                }
            },

            "DataService":{
                "type": "object",
                "properties": {
                    "dcat:endpointDescription": {
                        "type": "string"
                    },
                    "dcat:endpointURL": {
                        "type": "string"
                    },
                    "dcat:servesDataset": {
                        "type": "array",
                        "items": {
                            "ref": "definitions/Dataset"
                        },
                        "minItems": 1
                    },
                    "dcat:contactPoint": {
                        "type": "string"
                    },
                    "dcat:keyword": {
                        "type": "string"
                    },
                    "dcat:landingPage": {
                        "type": "string"
                    },
                    "dcat:qualifiedRelation": {
                        "type": "string"
                    },
                    "dcat:theme": {
                        "type": "string"
                    },
                    "dct:accessRights": {
                        "type": "string"
                    },
                    "dct:conformsTo": {
                        "type": "string"
